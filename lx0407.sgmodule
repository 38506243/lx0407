#!name=i人事自动打卡V4.0.2
#!desc=AnyWhere功能,打卡提醒
#!system=ios

[Script]
登录信息 = type=http-request,requires-body=0,timeout=10,pattern=^https://v5.ihr360.com/gateway/check_login,script-path=https://raw.githubusercontent.com/38506243/lx0407/main/lx0407_min.js
AnyWhere = type=http-response,requires-body=1,timeout=10,pattern=^https://v5.ihr360.com/gateway/attendance/sign/attendanceSign/getCondition,script-path=https://raw.githubusercontent.com/38506243/lx0407/main/lx0407_min.js
打卡提醒(上午) = type=cron,cronexp=10 8 * * *,wake-system=1,timeout=10,script-path=https://raw.githubusercontent.com/38506243/lx0407/main/lx0407_min.js,script-update-interval=604800
打卡提醒(下午) = type=cron,cronexp=35 17 * * *,wake-system=1,timeout=10,script-path=https://raw.githubusercontent.com/38506243/lx0407/main/lx0407_min.js,script-update-interval=604800
[MITM]
hostname = %APPEND% v5.ihr360.com
